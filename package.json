{
  "name": "@appwrite.io/synapse",
  "version": "0.2.1",
  "description": "Operating system gateway for remote serverless environments",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "test": "node --no-warnings --experimental-vm-modules node_modules/.bin/jest",
    "test:watch": "node --no-warnings --experimental-vm-modules node_modules/.bin/jest --watch",
    "test:coverage": "node --no-warnings --experimental-vm-modules node_modules/.bin/jest --coverage",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write .",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/appwrite/synapse.git"
  },
  "license": "MIT",
  "dependencies": {
    "node-pty": "^1.0.0",
    "ws": "^8.18.1",
    "eslint": "^9.23.0",
    "prettier": "^3.5.3"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.13",
    "@types/ws": "^8.18.0",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "esbuild": "^0.25.2",
    "esbuild-plugin-file-path-extensions": "^2.1.4",
    "jest": "^29.7.0",
    "ts-jest": "^29.3.0",
    "tsup": "^8.4.0",
    "typescript": "^5.8.2"
  },
  "bugs": {
    "url": "https://github.com/appwrite/synapse/issues"
  },
  "homepage": "https://github.com/appwrite/synapse#readme"
}
